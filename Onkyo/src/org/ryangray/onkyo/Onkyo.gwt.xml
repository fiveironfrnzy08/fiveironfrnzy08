<!--   <inherits name='com.google.gwt.libideas.LibIdeas' /> -->
<!--   <inherits name='com.google.gwt.gen2.Gen2' /> -->
<!--   <inherits name='com.google.gwt.gen2.widgetbase.WidgetBase' /> -->
<!--   <inherits name='com.google.gwt.gen2.widgetbase.client.Gen2CssInjector' /> -->
  <inherits name="com.smartgwt.SmartGwt" />
  
  
  <!-- Specify the app entry point class.                         -->
  <entry-point class='org.ryangray.onkyo.client.Onkyo'/>

  <!-- Specify the paths for translatable code                    -->
  <source path='client'/>
  <source path='shared'/>

</module>
